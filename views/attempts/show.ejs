<% layout('layouts/boilerplate') %>
<!-- Show score, graphs + analysis for attempt -->
<link rel="stylesheet" href="/stylesheets/styles.css" />
<a class="mt-5 px-4 py-2 font-medium tracking-wide text-white capitalize transition-colors duration-200 transform bg-indigo-600 rounded-md hover:bg-indigo-500 focus:outline-none focus:ring focus:ring-indigo-300 focus:ring-opacity-80 m-4 mb-8 p-4" href="/attempts"
  >&laquo; All Attempts</a
>
<h1 class="mt-3 text-2xl font-bold leading-7 text-gray-900 sm:text-3xl sm:truncate text-center">Attempt: <%= attempt.test.form %></h1>
<h2 class="my-3 text-xl font-bold leading-7 text-gray-600 sm:text-3xl sm:truncate text-center">
  <b>Date:</b> <%= attempt.dateTaken.toISOString().slice(0, 10)%>
</h2>

<div class="flex flex-col">
