<% layout('layouts/boilerplate') %>
<!-- Maybe review page??? -->
<h1>New Attempt</h1>
<form action="/attempts/<%= attempt._id %>?_method=PUT" method="POST">
  <div class="form-floating mb-3">
    <input
      class="form-control"
      type="text"
      id="date"
      name="attempt[dateTaken]"
      placeholder="MM/DD/YYYY"
      value="<%= attempt.date %>"
    />
    <label for="date">Date (mm/dd/yyyy)</label>
  </div>
  <h2>English</h2>
  <% for(let i=1;i<76;i++) { %>
  <div class="row">
    <div class="col">
      <div>
        <span class="fw-bold">Q<%= i %></span>
        <div class="form-check form-check-inline">
          <input type="radio" class="form-check-input" id="q<%= i %>A"
          name="attempt[answers][english][<%= i %>][choice]" value="A" <% if
          (attempt.answers.english[i - 1].choice == "A") { %> checked <% } %> />
          <label class="form-check-label" for="q<%= i %>A">A</label>
        </div>
        <div class="form-check form-check-inline">
          <input type="radio" class="form-check-input" id="q<%= i %>B"
          name="attempt[answers][english][<%= i %>][choice]" value="B" <% if
          (attempt.answers.english[i - 1].choice == "B") { %> checked <% } %> />
          <label class="form-check-label" for="q<%= i %>B">B</label>
        </div>
        <div class="form-check form-check-inline">
          <input type="radio" class="form-check-input" id="q<%= i %>C"
          name="attempt[answers][english][<%= i %>][choice]" value="C" <% if
          (attempt.answers.english[i - 1].choice == "C") { %> checked <% } %> />
          <label class="form-check-label" for="q<%= i %>C">C</label>
        </div>
        <div class="form-check form-check-inline">
          <input type="radio" class="form-check-input" id="q<%= i %>D"
          name="attempt[answers][english][<%= i %>][choice]" value="D" <% if
          (attempt.answers.english[i - 1].choice == "D") { %> checked <% } %> />
          <label class="form-check-label" for="q<%= i %>D">D</label>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="skipCheckbox<%= i
        %>" name="attempt[answers][english][<%= i %>][tags][0]" value="skip" <%
        if (attempt.answers.english[i - 1].tags.includes("skip")) { %> checked
        <% } %> />
        <label class="form-check-label" for="skipCheckbox<%= i %>">Skip</label>
      </div>
      <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="guessCheckbox<%= i
        %>" name="attempt[answers][english][<%= i %>][tags][1]" value="guess" <%
        if (attempt.answers.english[i - 1].tags.includes("guess")) { %> checked
        <% } %> />
        <label class="form-check-label" for="guessCheckbox<%= i %>"
          >Guess</label
        >
      </div>
      <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="timeCheckbox<%= i
        %>" name="attempt[answers][english][<%= i %>][tags][2]" value="noTime"
        <% if (attempt.answers.english[i - 1].tags.includes("noTime")) { %>
        checked <% } %> />
        <label class="form-check-label" for="timeCheckbox<%= i %>"
          >No Time</label
        >
      </div>
      <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="reviseCheckbox<%= i
        %>" name="attempt[answers][english][<%= i %>][tags][3]" value="revised"
        <% if (attempt.answers.english[i - 1].tags.includes("revised")) { %>
        checked <% } %> />
        <label class="form-check-label" for="reviseCheckbox<%= i %>"
          >Revised</label
        >
      </div>
    </div>
  </div>
  <% } %>
